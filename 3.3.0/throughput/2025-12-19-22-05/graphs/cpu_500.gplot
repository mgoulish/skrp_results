set terminal png size 1200, 1000
set output '../../../results/3.3.0/throughput/graphs/cpu_500.jpg'
set palette rgbformulae 33,13,10
set colorbox
set title "Version 3.3.0 : Throughput for CPU 500" font ",24" 
set xlabel "Router Threads" font ",20" 
set ylabel "Sender Threads" font ",20" 
set xtics font ",16" ("1" 0, "2" 1, "4" 2, "5" 3, "7" 4, "10" 5, )
set ytics font ",16" ("1" 0, "2" 1, "5" 2, "10" 3, )
set cbrange [0.7696666666666667:19.166666666666668]
set autoscale xfix
set autoscale yfix
set autoscale cbfix
plot '../../../results/3.3.0/throughput/graphs/cpu_500.data' matrix with image title "", \
     ""     matrix using 1:2:(sprintf('%.1f', $3)) with labels center font ",16"